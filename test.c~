#include "nodes.h"
#include <stdlib.h>

int		create_bug(t_nd *position, int i)
{
  t_ant		*bug;

  bug = malloc(sizeof(t_ant));
  bug->number = i;
  bug->node = position;
  printf("%d\n", bug->number);
}

int		my_create(int nb, t_nd *position)
{
  int		i;

  i = 0;
  while (i < nb)
    {
      create_bug(position, i);
      i++;
    }
}

int		main()
{
  t_nd		*position;

  position = malloc(sizeof(t_nd));
  my_create(3, position);
}
